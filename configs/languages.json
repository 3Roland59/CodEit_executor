{
  "python": {
    "image": "python:3.10",
    "extension": "py",
    "command": "python /code/Main.py"
  },
  "javascript": {
    "image": "node:18",
    "extension": "js",
    "command": "node /code/Main.js"
  },
  "typescript": {
    "image": "node:18",
    "extension": "ts",
    "command": "npx ts-node /code/Main.ts"
  },
  "java": {
    "image": "openjdk:17",
    "extension": "java",
    "command": "javac /code/Main.java && java -cp /code Main"
  },
  "go": {
    "image": "golang:1.20",
    "extension": "go",
    "command": "go run /code/main.go"
  },
  "cpp": {
    "image": "gcc:latest",
    "extension": "cpp",
    "command": "g++ /code/main.cpp -o /code/app && /code/app"
  }
}

